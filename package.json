{
  "name": "@3xpedia/library-boilerplate",
  "version": "0.2.0",
  "description": "Simple NodeJs library boilerplate. ESM to anything else.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "targets": {
    "main": {
      "source": "./index.cjs",
      "includeNodeModules": false
    },
    "module": {
      "source": "./index.mjs",
      "includeNodeModules": false
    },
    "global": {
      "source": "./index.browser.js",
      "context": "browser",
      "outputFormat": "global",
      "includeNodeModules": true,
      "optimize": true
    }
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "scripts": {
    "prebuild": "del-cli ./dist",
    "build": "parcel build",
    "watch": "parcel watch"
  },
  "author": {
    "name": "Coclet Cl√©ment",
    "url": "https://github.com/3xpedia"
  },
  "keywords": [
    "javascript",
    "library",
    "boilerplate",
    "esm",
    "parcel"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3xpedia/library-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/3xpedia/library-boilerplate/issues"
  },
  "devDependencies": {
    "del-cli": "^5.0.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "parcel": "^2.8.2",
    "prettier": "^2.8.3"
  }
}
